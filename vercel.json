{
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/iv-scanner?type=OPEN",
      "schedule": "49 13 * * 1-5"
    },
    {
      "path": "/api/iv-scanner?type=CLOSE",
      "schedule": "41 19 * * 1-5"
    },
    {
      "path": "/api/gex-background-scan?type=OPEN",
      "schedule": "47 13 * * 1-5"
    },
    {
      "path": "/api/gex-background-scan?type=CLOSE",
      "schedule": "43 19 * * 1-5"
    },
    {
      "path": "/api/otm-background-scan?type=PREMARKET",
      "schedule": "14 13 * * 1-5"
    },
    {
      "path": "/api/otm-background-scan?type=MIDDAY",
      "schedule": "30 16 * * 1-5"
    },
    {
      "path": "/api/otm-background-scan?type=CLOSE",
      "schedule": "44 19 * * 1-5"
    }
  ],
  "functions": {
    "src/app/api/options-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/live-options-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/stream-options-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/gex-screener/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/sweep-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/polygon-options/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/market-sentiment/route.ts": {
      "maxDuration": 180
    },
    "src/app/api/preloader/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/iv-scanner/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/hv-screener/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/gex-background-scan/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/otm-background-scan/route.ts": {
      "maxDuration": 300
    }
  },
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods", 
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}