{
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/gex-scan",
      "schedule": "*/10 13-20 * * 1-5"
    },
    {
      "path": "/api/cron/otm-scan",
      "schedule": "*/13 13-20 * * 1-5"
    }
  ],
  "functions": {
    "src/app/api/options-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/live-options-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/stream-options-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/gex-screener/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/sweep-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/polygon-options/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/market-sentiment/route.ts": {
      "maxDuration": 180
    },
    "src/app/api/preloader/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/iv-scanner/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/hv-screener/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/historical-volatility/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/iv-history/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/spx-debug/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/spx-fix/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/spx-test/route.ts": {
      "maxDuration": 300
    }
  },
  "installCommand": "npm install",
  "buildCommand": "npm run vercel-build",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production",
      "SKIP_ENV_VALIDATION": "1"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods", 
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}