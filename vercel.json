{
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/background-screeners",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/cron/spy-algoflow",
      "schedule": "*/5 * * * *"
    }
  ],
  "functions": {
    "src/app/api/options-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/live-options-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/stream-options-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/gex-screener/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/sweep-flow/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/polygon-options/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/market-sentiment/route.ts": {
      "maxDuration": 180
    },
    "src/app/api/preloader/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/background-screeners/route.ts": {
      "maxDuration": 600
    },
    "src/app/api/cron/spy-algoflow/route.ts": {
      "maxDuration": 300
    }
  },
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods", 
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}